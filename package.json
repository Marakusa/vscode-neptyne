{
    "name": "vscode-neptyne",
    "displayName": "Neptyne",
    "description": "Neptyne is a simple programming language",
    "icon": "icons/icon128.png",
    "author": "Markus Kannisto",
    "version": "0.0.1",
    "publisher": "Marakusa",
    "repository": "https://github.com/Marakusa/vscode-neptyne",
    "bugs": "https://github.com/Marakusa/vscode-neptyne/issues",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "neptyne",
            "aliases": ["Neptyne", "neptyne"],
            "extensions": [".npt",".nptp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "neptyne",
            "scopeName": "source.neptyne",
            "path": "./syntaxes/neptyne.tmLanguage.json"
        }],
        "snippets": [{
            "language": "neptyne",
            "path": "./snippets/neptyne.json"
        }]
    }
}